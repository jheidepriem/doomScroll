

{
  "expo": {
    "name": "doomScroll",
    "slug": "doomScroll",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/doomLogo5.png",
      "resizeMode": "contain",
      "backgroundColor": "#020d19"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.turing.doomScroll",
      "supportsTablet": true
 
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "black"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
